{
  "name": "we-love-cake",
  "version": "1.0.0",
  "description": "days since last cake",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "copy": "mkdir build && cp app/index.html build",
    "build": "npm run clean && npm run copy && browserify app/cake.ts -p [ tsify ] >build/cake.js",
    "watch": "watchify app/cake.ts -p [ tsify ] -o build/cake.js --debug --verbose"
  },
  "author": "Mike Arvela <mike@arvela.net>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "tsify": "^0.16.0",
    "typescript": "^1.8.10",
    "watchify": "^3.7.0"
  }
}
